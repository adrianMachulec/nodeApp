<%- include('partials/header') -%>
    
    <% if(name){ %>
        <h1>Firma: <%= name %></h1>
    <% } else { %>
        <h1>Nie ma takiej firmy</h1>
    <% } %>

    <ul>
        <% companies.forEach(company => { %>
        <li><a href="/firmy/<%= company.slug %>"><%= company.slug %></a></li>
        <% }) %>
    </ul>

<%- include('partials/footer') -%>